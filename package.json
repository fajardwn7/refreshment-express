  "scripts": {
    "start": "ts-node-dev --respawn --transpile-only --exit-child src/app.ts",
    "build": "tsc -p .",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrate": "rm -rf dist && yarn build && yarn typeorm migration:generate ./src/migrations/added-entity -d ./src/utils/dataSource.ts",
    "db:push": "rm -rf dist && yarn build && yarn typeorm migration:run -d src/utils/dataSource.ts",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix ."
  }
